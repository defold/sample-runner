[project]
title = Runner
dependencies#0 = https://github.com/defold/extension-spine/archive/refs/tags/1.0.5.zip

[bootstrap]
main_collection = /main/main.collectionc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 1280
height = 720

[physics]
scale = 0.02
debug = 0

[graphics]
default_texture_min_filter = linear
default_texture_mag_filter = linear

[script]
shared_state = 1

[particle_fx]
max_count = 1024
max_particle_count = 2048

[library]
include_dirs = def-runner

